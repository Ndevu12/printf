#include "main.h"
#include <stdarg.h>
#include <stdio.h>
#include <unistd.h>

int _printf(const char *format, ...);
{
	va_list list;
	int task = 0;

	if (format == NULL)
	{
		return -1
	}
	va_start(list, format);

	while (*format)
	{
		if (*format != '%')
		{
			_putchar(*format);
			task++;
	}
	else
	{
		format++;
		if (*formart == '\0')
		{
			break
	}
	else if (*formart == 'd' || *format == 'i')
	{
		int num = va_arg(list, int);
		task += _print_int(num);
	}
	format++;
	}
	va_end(list);
	return (task);
}
